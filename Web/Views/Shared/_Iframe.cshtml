@{
    var isPrintPage = TempData["isPrintPage"];
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/favicon.ico" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrap", "~/Content/admincss")
    @Styles.Render("~/Content/bootstraptablecss")
    @Styles.Render("~/bundles/jqueryfancytreecss")
    @Styles.Render("~/bundles/jquerymagnificecss")
    @Styles.Render("~/bundles/jqueryfinduploadercss")
    @Styles.Render("~/bundles/datetimepickercss")
    <link href="~/Scripts/layer/skin/layer.css" rel="stylesheet" />
    @Scripts.Render("~/Scripts/ueditor/ueditorjs")
    @Scripts.Render("~/Scripts/adminjs", "~/Scripts/thirdpartyjs", "~/Scripts/layer/layerjs")
    @Scripts.Render("~/Scripts/bootstraptablejs")
    @Scripts.Render("~/bundles/jqueryfancytreejs")
    @Scripts.Render("~/bundles/jquerymagnificejs")
    @Scripts.Render("~/bundles/jqueryfinduploaderjs")
    @Scripts.Render("~/bundles/datetimepickerjs")
    <style>
        .fancybox img {
            margin-bottom: 5px;
            width: 6.3%;
        }
    </style>
</head> 
<body class="teacher-index">
    <div class="wrapper wrapper-content animated fadeInRight">
        @RenderBody()
    </div>
    @RenderSection("scripts", false)
    <script>
        $(function () {
            if ('@isPrintPage' != 'true') {
                $(document).keydown(function () {
                    if (window.parent.resetTime) {
                        window.parent.resetTime();
                    }
                });
                $(document).click(function () {
                    if (window.parent.resetTime) {
                        window.parent.resetTime();
                    }
                });
            }

            //if (/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
            //    $('.selectpicker').selectpicker('mobile');
            //}

            $(window.parent.document).find(".second-level-active").children().click(function (e) {
                e.preventDefault();
                if (!window) {
                    return;
                }
                var ifrmaeUrl = ($(window.parent.document).find(".second-level-active").children().attr("href"));
                var iframe = $(window.parent.document).find("iframe[src ='" + ifrmaeUrl + "']");
                iframe.attr("src", iframe.attr("src"));
            })
        });
    </script>
</body>
</html>
